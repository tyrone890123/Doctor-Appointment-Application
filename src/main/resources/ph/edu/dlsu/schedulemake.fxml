<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" stylesheets="@stylesheets/diagnosis.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ph.edu.dlsu.ScheduleMakeController">
   <children>
      <Region fx:id="bar" layoutY="-1.0" prefHeight="26.0" prefWidth="400.0" />
      <Label layoutX="88.0" layoutY="31.0" text="Create Schedule" textFill="WHITE">
         <font>
            <Font name="Script MT Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="save" layoutX="338.0" layoutY="254.0" mnemonicParsing="false" text="Save" />
      <TextField fx:id="timeIn" layoutX="163.0" layoutY="95.0" promptText="HH:MM" />
      <TextField fx:id="timeOut" layoutX="163.0" layoutY="137.0" promptText="HH:MM" />
      <ComboBox fx:id="intervalChoose" layoutX="162.0" layoutY="183.0" prefWidth="150.0" promptText="Choose Interval" />
      <Label layoutX="105.0" layoutY="99.0" text="Time In" />
      <Label layoutX="105.0" layoutY="141.0" text="Time Out" />
      <Label layoutX="105.0" layoutY="187.0" text="Interval" />
      <Button fx:id="exit" layoutX="374.0" layoutY="1.0" mnemonicParsing="false" onAction="#exit" style="-fx-background-color: transparent;" text="X" textFill="WHITE" />
   </children>
</AnchorPane>
